{
  "expo": {
    "name": "QuizApp",
    "description": "A simple quiz application built with React Native and Expo",
    "slug": "quiz2",
    "privacy": "public",
    "sdkVersion": "27.0.0",
    "platforms": ["android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.QuizApp"
    }
  }
}
